<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="examples.SongController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Song Example Form" />

      <HBox alignment="BASELINE_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="BASELINE_LEFT" prefHeight="16.0" prefWidth="80.0" text="Song name">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="nameText"  prefHeight="24.0" prefWidth="266.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>

      <HBox alignment="BASELINE_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="BASELINE_LEFT" prefHeight="16.0" prefWidth="80.0" text="URL">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="urlText"  prefHeight="24.0" prefWidth="266.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>




      <HBox alignment="BASELINE_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="BASELINE_LEFT" prefHeight="16.0" prefWidth="80.0" text="Artist">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="artistText"  prefHeight="24.0" prefWidth="266.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>




      <HBox alignment="BASELINE_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="BASELINE_LEFT" prefHeight="16.0" prefWidth="80.0" text="User">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="userText"  prefHeight="24.0" prefWidth="266.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>


      <HBox alignment="BASELINE_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label alignment="BASELINE_LEFT" prefHeight="16.0" prefWidth="80.0" text="Tags">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Label>
            <TextField fx:id="tagsText"  prefHeight="24.0" prefWidth="266.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>


      <Button mnemonicParsing="false" onAction="#doAdd" text="Add" />
   </children>
</VBox>
